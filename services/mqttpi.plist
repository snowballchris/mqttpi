  <dict>
    <key>MQTT_BROKER</key>
    <string>homeassistant.local</string>
    <key>MQTT_PORT</key>
    <string>1883</string>
  </dict>
  <key>StandardOutPath</key>
  <string>/Users/chnes/Library/Logs/mqttpi.out.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/chnes/Library/Logs/mqttpi.err.log</string>
</dict>
</plist>

